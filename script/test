# Run basic tests on base.

set -o errexit


# Set script folder
SCRIPT_DIR="$( cd "$(dirname "${BASH_SOURCE[0]:-$0}")" >/dev/null 2>&1 ; pwd -P )"

# Set root folder
PROJ_DIR="$( dirname "${SCRIPT_DIR}" )"

result_all=0

pushd "${PROJ_DIR}"
    # No tests defined
popd

exit $result_all
